<!DOCTYPE html>
<html lang="en">


<!-- Mirrored from appstack.bootlab.io/ui-carousel.html by HTTrack Website Copier/3.x [XR&CO'2014], Tue, 22 Nov 2022 19:15:37 GMT -->
<!-- Added by HTTrack --><meta http-equiv="content-type" content="text/html;charset=UTF-8" /><!-- /Added by HTTrack -->
<%- include('../Partes/head')  %>
<!--
  HOW TO USE: 
  data-theme: default (default), dark, light
  data-layout: fluid (default), boxed
  data-sidebar-position: left (default), right
  data-sidebar-behavior: sticky (default), fixed, compact
-->

<body data-theme="default" data-layout="fluid" data-sidebar-position="left" data-sidebar-behavior="sticky">
	<div class="wrapper">
		<%- include('../Partes/sidebar')  %>
		<div class="main">
			<%- include('../Partes/nav')  %>

			<main class="content">
				<div class="container-fluid p-0">
					<div class="row">
						<% produtos.reverse().slice(0,4).map(produto=>{ %>
						<div class="col-12 col-md-6 col-lg-3">
							<div class="card">
								<div id="carouselExampleFade<%=produto.idCompra%>" class="carousel slide carousel-fade" data-bs-ride="carousel">
									<div class="carousel-inner">
										<div class="carousel-item active">
											<img class="d-block w-100" src="/uploads/foto/<%=produto.imagenProduto1%>" alt="Unsplash">
										</div>
										<div class="carousel-item">
											<img class="d-block w-100" src="/uploads/foto/<%=produto.imagenProduto2%>" alt="Unsplash">
										</div>
										<div class="carousel-item">
											<img class="d-block w-100" src="/uploads/foto/<%=produto.imagenProduto3%>" alt="Unsplash">
										</div>
										<div class="carousel-item">
											<img class="d-block w-100" src="/uploads/foto/<%=produto.imagenProduto4%>" alt="Unsplash">
										</div>
										<div class="carousel-item">
											<img class="d-block w-100" src="/uploads/foto/<%=produto.imagenProduto5%>" alt="Unsplash">
										</div>
									</div>
									<a class="carousel-control-prev" href="#carouselExampleFade<%=produto.idCompra%>" role="button" data-bs-slide="prev">
		<span class="carousel-control-prev-icon" aria-hidden="true"></span>
		<span class="sr-only">Previous</span>
	  </a>
									<a class="carousel-control-next" href="#carouselExampleFade<%=produto.idCompra%>" role="button" data-bs-slide="next">
		<span class="carousel-control-next-icon" aria-hidden="true"></span>
		<span class="sr-only">Next</span>
	  </a>
								</div><div class="card-header">
									<h5 class="card-title mb-0"><%=produto.nomeProduto%></h5>
								</div>
								<div class="card-body">
									<p class="card-text"><%=produto.infoProduto%>.</p>
									<a href="#" class="btn btn-primary">Ver Mais...</a>
								
								</div>
							</div>
						</div>
						<%}) %>
						</div>
					<div class="row">
						<div class="col-12">
							<div class="card">
								
								<div class="card-body">
									<table id="datatables-buttons" class="table table-striped" style="width:100%">
										<thead>
											<tr>
												<th>#</th>
												<th>Name</th>
												<th>Quantidade Adquirido</th>
												<th>Preço Unitário</th>
												<th>Data Compra</th>
												<th>Preço Total</th>
												<th>Acção</th>
											</tr>
										</thead>
										<tbody>
											<% produtos.reverse().map(produto=>{ %>
											<tr>
												<td><%=produto.idCompra%></td>
												<td><%=produto.nomeProduto%></td>
												<td><%=produto.quantidade_produto%> Unidade(s)</td>
												<td><%=produto.precoProduto%>  kz</td>
												<td><%=produto.dataCompra%></td>
												<td><%=produto.preco_pagar%>  kz</td>
												<td>
													<div class="btn-group btn-group-sm">
														<i class="align-middle dropdown-toggle" data-feather="list" data-bs-toggle="dropdown" aria-haspopup="true"
														aria-expanded="false"></i>
														
														<div class="dropdown-menu">
															<a class="dropdown-item" href="#">Ver</a>
															<div class="dropdown-divider"></div>
															<a class="dropdown-item" href="#">Outros</a>
														</div>
													</div>
													
												</td>
											</tr>
											<%}) %>
											
										</tbody>
									</table>
								</div>
							</div>
						</div>
						
						
					</div>
					

				</div>
			</main>

			<%- include('../Partes/footer')  %> 
		</div>
	</div>

	<script src="/Dash/js/app.js"></script>
	<script src="/sweetalert/sweetalert2.all.min.js"></script>
	<script>
		document.addEventListener("DOMContentLoaded", function() {
			// Datatables with Buttons
			var datatablesButtons = $("#datatables-buttons").DataTable({
				responsive: true,
				lengthChange: !1,
				buttons: ["copy", "print"]
				
			});
			datatablesButtons.buttons().container().appendTo("#datatables-buttons_wrapper .col-md-6:eq(0)");
		});
	</script>

</body>


